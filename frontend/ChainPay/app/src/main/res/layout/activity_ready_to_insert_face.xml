<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/register_insert_text_up"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textSize="20sp"
        android:textColor="#a69c49"
        android:layout_marginTop="100dp"
        android:text="接下来进行录入您的人脸数据"/>

    <TextView
        android:id="@+id/register_insert_text_down"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textSize="20sp"
        android:textColor="#a69c49"
        android:text="点击Go开始拍摄"/>


    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="75pt"
        android:layout_marginTop="70pt"
    android:orientation="vertical"
    android:gravity="center">


        <Button
        android:id="@+id/register_insert_go_camera"
        android:layout_width="77pt"
        android:layout_height="wrap_content"
        android:text="Go"/>

    </LinearLayout>


</LinearLayout>